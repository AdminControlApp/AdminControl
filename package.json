{
	"name": "admin-control-monorepo",
	"type": "module",
	"private": true,
	"engines": {
		"pnpm": ">=7.0.0"
	},
	"scripts": {
		"setup": "node ./scripts/setup.js",
		"pnpm:devPreinstall": "node ./scripts/setup-check.js",
		"prepare": "lion-git-hooks",
		"tc": "pnpm recursive run tc",
		"lint": "pnpm recursive run lint",
		"prettier": "pnpm recursive run prettier",
		"dev:electron": "pnpm --filter=@admincontrol/app run dev",
		"build:electron": "pnpm --filter=@admincontrol/app run build",
		"compile:electron": "pnpm --filter=@admincontrol/app run compile"
	},
	"devDependencies": {
		"@leonzalion/configs": "^1.2.33",
		"@types/node": "^17.0.30",
		"eslint-define-config": "^1.4.0",
		"execa": "^5.1.1",
		"lion-system": "^1.3.22",
		"typescript": "^4.6.4"
	}
}
