{
	"name": "@admincontrol/app",
	"type": "commonjs",
	"description": "An open-source application that controls administrator access to your computer.",
	"author": {
		"name": "Leon Si",
		"email": "contact@leonzalion.com",
		"url": "https://leonzalion.com"
	},
	"repository": "AdminControlApp/AdminControl",
	"main": "main/dist/main.cjs",
	"scripts": {
		"build": "pnpm run build:main && pnpm run build:preload && pnpm run build:renderer",
		"build:main": "cd ./main && vite build",
		"build:preload": "cd ./preload && vite build",
		"build:renderer": "cd ./renderer && vite build",
		"create-icons": "node-ts ./scripts/create-icons.ts",
		"compile": "node-ts ./scripts/compile.ts",
		"test": "pnpm run test:main && pnpm run test:preload && pnpm run test:renderer && pnpm run test:e2e",
		"test:e2e": "pnpm run build && vitest run",
		"test:main": "vitest run -r main --passWithNoTests",
		"test:preload": "vitest run -r preload --passWithNoTests",
		"test:renderer": "vitest run -r renderer --passWithNoTests",
		"dev": "cross-env ELECTRON_RUN_AS_NODE='' node-ts ./scripts/watch.ts",
		"typecheck:main": "tsc --noEmit -p main/tsconfig.json",
		"typecheck:preload": "tsc --noEmit -p preload/tsconfig.json",
		"typecheck:renderer": "vue-tsc --noEmit -p renderer/tsconfig.json",
		"typecheck": "pnpm run typecheck:main && pnpm run typecheck:preload && pnpm run typecheck:renderer"
	},
	"dependencies": {
		"@admincontrol/encryption": "link:../encryption/dist",
		"@bitwarden/cli": "^1.22.1",
		"@commonjs/execa": "npm:execa@^5.1.1",
		"@leonzalion/p-wait-for": "npm:@leonzalion/p-wait-for@^4.2.1",
		"applescript-utils": "^0.1.20",
		"delay": "^5.0.0",
		"desm": "^1.2.0",
		"electron-store": "^8.0.1",
		"electron-updater": "^5.0.1",
		"fast-deep-equal": "^3.1.3",
		"fast-sha256": "^1.3.0",
		"get-secure-input-processes": "^0.0.1",
		"is-ci": "^3.0.1",
		"keytar": "^7.9.0",
		"ldebug": "^1.0.0",
		"nanoid-nice": "^1.0.1",
		"onetime": "^6.0.0",
		"package-name-regex": "^2.0.6",
		"phone-call-input": "link:../phone-call-input/dist",
		"pinia": "^2.0.14",
		"random-int": "^3.0.0",
		"rollup-plugin-js-imports": "^1.2.1",
		"sudo-prompt": "^9.2.1",
		"vue": "^3.2.35",
		"vue3-notify": "^0.0.2",
		"vue3-spinners": "^1.2.1",
		"windicss": "^3.5.4"
	},
	"devDependencies": {
		"@rollup/plugin-node-resolve": "^13.3.0",
		"@types/is-ci": "^3.0.0",
		"@types/node": "^17.0.35",
		"@types/package-name-regex": "^2.0.0",
		"@types/sharp": "^0.30.2",
		"@vitejs/plugin-vue": "2.3.1",
		"@vue/test-utils": "2.0.0-rc.21",
		"cross-env": "7.0.3",
		"electron": "18.2.4",
		"electron-builder": "23.0.3",
		"electron-devtools-installer": "3.2.0",
		"happy-dom": "2.55.0",
		"playwright": "1.21.1",
		"rollup": "^2.74.1",
		"rollup-plugin-bundle-esm": "^0.1.1",
		"sharp": "^0.30.4",
		"typescript": "^4.6.4",
		"vite": "^2.9.9",
		"vite-plugin-windicss": "^1.8.4",
		"vitest": "0.9.4",
		"vue-tsc": "0.34.9"
	}
}